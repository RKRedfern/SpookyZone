<h2>Edit Your Comment: </h2>

<%= render partial: "layouts/errors", locals: {object: @comment} %>

<%= form_for(@comment) do |f|%>

    <%= f.label :story %>
    <%= f.collection_select :story_id, Story.all, :id, :title %>

    <%= f.label :content %>
    <%= f.text_field :content %>


    <%= f.submit %>

<% end %>

